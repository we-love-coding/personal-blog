<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.barnnet.work.helper.mapper.RegisterMapper">

    <insert id="registerByPhone">
        insert into loginuser(phone,user_password) values (#{phone},#{password});
    </insert>

    <insert id="registerByEmail">
        insert into loginuser(email,user_password) values (#{email},#{password});
    </insert>

    <insert id="registerByUser">
        insert into loginuser(user_name,user_password) values (#{user},#{password});
    </insert>
</mapper>